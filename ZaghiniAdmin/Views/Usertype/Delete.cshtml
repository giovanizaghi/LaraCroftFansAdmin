@model ZaghiniAdmin.Models.usertype

@{
    ViewBag.Title = "Delete";
}

<div class="z_breadcumb">
    <a href="@Url.Action("Index","UserType")">User Types</a><i class="fa fa-angle-double-right fa-fw"></i><a href="#">Delete User Type</a>
</div>


<div class="row">
    <div class="z_panel">
        <div class="z_panel-title">
            <h4>Delete User Type</h4>
            <div class="clearfix"></div>
        </div>
        <div class="z_panel-content">
            <dl class="dl-horizontal">
                <dt>@Html.DisplayName("Description")</dt>
                <dd>@Html.DisplayFor(model => model.description)</dd>
            </dl>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </div>
            }
        </div>
    </div>
</div>





